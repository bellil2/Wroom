<div id="PartHa">
  <img src="/image/cat_circuit.png" alt="ImageDeDrapeau">
  <h1>{{title}}</h1>
  <img src="/image/act_add.png" alt="modif">
  <a href="/ajouterCircuit"> Ajouter un circuit</a>

  <table>
    <tr>
      <th>Nom</th>
      <th>Longueur</th>
      <th>Nombre de spectateurs</th>
      <th>Actions</th>
    </tr>
    {{#each listeCircuits}}
    <tr>
      <td>{{this.CIRNOM}}</td>
      <td>{{this.CIRLONGUEUR}} m</td>
      <td>{{this.CIRNBSPECTATEURS}}</td>
      <td>
        <img src="/image/act_delete.png" alt="modif">
        <a href="/supprimerCircuit/{{this.CIRNUM}}"> Supprimer</a>
      </td>
    </tr>
    {{/each}}
  </table>
</div>